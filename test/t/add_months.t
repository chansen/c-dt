#!perl
use strict;
use warnings;

use Test::More 0.88;
use dtp qw[:all];

while (<DATA>) {
    next if /\A \# /x;
    chomp;
    my ($y, $m, $d, $delta, $adjust, $ey, $em, $ed) = split /,\s*/, $_;

    {
        my $dt = dt_from_ymd($y, $m, $d);
        my $exp = sprintf '%.4d-%.2d-%.2d', $ey, $em, $ed;
        my $got = sprintf '%.4d-%.2d-%.2d', dt_to_ymd(dt_add_months($dt, $delta, $adjust));
        is($got, $exp, "dt_add_months($dt, $delta, $adjust)");
    }

    if ($adjust != 0) {
        my $dt1 = dt_from_ymd($y, $m, $d);
        my $dt2 = dt_add_months($dt1, $delta, $adjust);
        my $got = dt_delta_months($dt1, $dt2);
        is($got, $delta, sprintf '%.4d-%.2d-%.2d -- %.4d-%.2d-%.2d', $y, $m, $d, $ey, $em, $ed);
    }
}

done_testing;

